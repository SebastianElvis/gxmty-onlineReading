<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <constant name="struts.devMode" value="true" />
	
    <package name="main" namespace="/Main" extends="struts-default">
		<action name="Index" class="main.Index" method="execute">
			<result>/index.jsp</result>
		</action>
		<action name="About">
			<result>/about.jsp</result>
		</action>
		<action name="Contact">
			<result>/contact.jsp</result>
		</action>
    </package> 
    <package name="reading" namespace="/Reading" extends="struts-default">
    	<action name="List">
    		<result>/list.jsp</result>
    	</action>
    </package>
	
    <!-- Add packages here -->

</struts>
